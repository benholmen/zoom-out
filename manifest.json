{
    "manifest_version": 3,
    "name": "Zoom Out",
    "version": "1.0",
    "description": "Automatically closes Zoom meeting tabs.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Zoom Out"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
